AT_SETUP([Ambiguous AND/OR])

AT_DATA([prog.cob], [
       IDENTIFICATION   DIVISION.
       PROGRAM-ID.      prog.
       PROCEDURE        DIVISION.
           IF 3 = 1 AND 2 OR 3
               DISPLAY "OK"
           END-IF.
           IF 3 = 1 OR 2 AND 3
               DISPLAY "NO"
           END-IF.
           STOP RUN.
])

AT_CHECK([${COMPILE_ONLY} prog.cob], [0], ,
[prog.cob:6: warning: suggest parentheses around AND within OR
prog.cob:9: warning: suggest parentheses around AND within OR
])

AT_CLEANUP
