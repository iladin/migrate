AT_INIT([])

AT_TESTED([cobc])

# Fundamental tests before anything else
m4_include([fundamental.at])

# ISO/IEC 1989:2002(E) COBOL standard tests
m4_include([subscripts.at])	# 8.4.1.2 Subscripts
m4_include([ref-mod.at])	# 8.4.2.3 Reference-modification

m4_include([redefines.at])	# 13.16.42 REDEFINES clause
m4_include([usage.at])		# 13.16.58 USAGE clause
m4_include([value.at])		# 13.16.61 VALUE clause

m4_include([initialize.at])	# 14.8.19 INITIALIZE statement
m4_include([move.at])		# 14.8.24 MOVE statement

# Other tests
m4_include([definition.at])
m4_include([extension.at])
m4_include([entry.at])
m4_include([misc.at])
